<VBox alignment="CENTER" prefHeight="478.0" prefWidth="431.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.SecondaryController">
    <children>
      <ImageView fitHeight="67.0" fitWidth="82.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../imagenes/desencriptar.png" />
         </image>
      </ImageView>
      <Pane prefHeight="519.0" prefWidth="391.0">
         <children>
              <Button layoutX="229.0" layoutY="310.0" onAction="#switchToPrimary" text="Volver" />
            <TextArea fx:id="textoEncriptar" prefHeight="138.0" prefWidth="391.0" promptText="Ingrese el texto que desea proteger" />
            <Button layoutX="97.0" layoutY="310.0" onAction="#btnencriptar" text="Encriptar" />
            <Label fx:id="textoEncriptado" layoutX="3.0" layoutY="143.0" prefHeight="158.0" prefWidth="391.0" />
         </children>
      </Pane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
